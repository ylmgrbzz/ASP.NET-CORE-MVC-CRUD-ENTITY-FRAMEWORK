@model List<ASPNETMVCCRUD.Models.Domain.Employee>
@{
}

<table class="table">
    <thead>
<tr>
            <th>
                Name
            </th>
            <th>
                Email
            </th>
            <th>
                Salary
            </th>
            <th>
                Date of birth
            </th>
            <th>
                Department
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>
                    @employee.Name
                </td>
                <td>
                    @employee.Email
                </td>
                <td>
                    @employee.Salary
                </td>
                <td>
                    @employee.DateOfBirth.ToString("dd/MM/yyyy")
                </td>
                <td>
                    @employee.Department
                </td>
            </tr>
        }
    </tbody>
</table>